{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-project-template",
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "version": "0.0.1",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [],
  "ppx-flags": [],
  "refmt": 3,
  "warnings": {
    "error": "+101"
  },
}